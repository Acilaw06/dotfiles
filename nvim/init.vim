lua require('plugins')

"theme
colorscheme onedark

set number
filetype on
filetype indent on
syntax on
set cursorline
set history=100
set shiftwidth=4
set tabstop=4
set scrolloff=4
set expandtab
set nobackup
set nowrap
set showmatch
set hlsearch


set wildmenu
set completeopt=menu,menuone,noselect
set wildmode=longest:full,list:full
set wildignore =*.o,*.obj,*.docx

